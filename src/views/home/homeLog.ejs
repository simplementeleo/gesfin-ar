    <title>Gesfin</title>
</head>

<body>
    <div class="container">

        <div class="navegacion">
            <div class="gesfinHome">
                <img src="/img/icono/MenuNav.svg" class="menuMarket" alt="Menu">
                <img src="/img/icono/flechaBarra.svg" class="logoPrincipalMarket" alt="Logo">
                <div id="letraLogoMarket">
                    <div>
                        <h4>GesFin</h4>

                    </div>
                </div>
            </div>
            <div class="medio">
                <% if (error.length> 0) {%>
                    <div class="cartelError">
                        <%= error %>
                    </div>
                    <%}%>
            </div>
            <div class="user">
                <img src="/img/icono/campana.png" alt="campana" class="campana" title="Notificaciones">
                <img src="/img/icono/user.png" alt="perfil" class="perfil" title="Usuario">
                <nav>

                    <ul class="men-prin">
                        <li>
                            <h3 id="usuarioDes">
                                <%= userNombre %>
                                    </>
                                    <img src="/img/icono/triangAbajo.png" class="fechaDown">
                            </h3>


                            <ul class="men">
                                <li><a href="">Configuración de usuario</a></li>
                                <li><a href="marketp">Configuracion Aplicacion</a></li>
                                <li><a href="/logout ">Cerrar sesión</a></li>
                            </ul>

                        </li>

                    </ul>
                </nav>

            </div>
        </div>


    </div>
    <div id="menu-container" class="menu-container">
        <div class="nav-vert">
            <nav>
                <ul>
                    <li><img src="/img/icono/bug.svg" alt="Error" title="Carga de errores" class="error"></li>
                </ul>
                <ul>
                    <li><img src="/img/icono/reque.svg" alt="Requerimiento" title="Carga de requerimientos"
                            class="reque"></li>
                </ul>
                <ul>
                    <li><img src="/img/icono/menu.png" alt="Menu" title="Panel de control" class="panel"></li>
                </ul>

            </nav>

        </div>
        <div class="nav-completa">
            <nav>
                <ul>

                    <li>
                        <p class="cobb desplegable" menu="subMenuCob" img="reque">Errores</span></p>
                        <ul class="subMenuCob">
                            <li>
                                <p class="menuFormulario" id="error">Ingresar Error</p>
                            </li>
                            <li>
                                <p class="menuSelectAbm" id="error">Errores ingresados</p>
                            </li>
                        </ul>
                    </li>

                    <li>
                        <p class="pagg desplegable" menu="subMenuPag" img="panel">Requerimientos</p>
                        <ul class="subMenuPag">
                            <li>
                                <p class="menuFormulario" id="requerimiento">Ingresar Requerimiento</p>
                            <li>
                                <p class="menuSelectAbm" id="requerimiento">Requerimientos ingresados</p>
                            </li>
                        </ul>
                    </li>


                    <li class="panel">Panel de control</li>
                </ul>
            </nav>

        </div>

        <div id="tablas" class="comp">
            <div class="tabs_links" id="tabs_links"></div>
            <div class="comandos" id="comandera"></div>
            <div class="tabs_contents" id="tabs_contents">
            </div>

        </div>

    </div>
    <div class="logoFondo">
        <!-- <img src="/img/logo12.png" alt="">-->
    </div>

    </div>

    <div>
        <input id="oculto" value="<%=username %>">

    </div>

    <!-- Creacion formulario individual-->

    <div id="formularioIndividual2"></div>
    <div id="formularioIndividual">

        <div class="cuadroForm">
            <div class="primeraLinea">

                <div id="comanderaIndiv">
                </div>


            </div>
            <div class="logoForm">
                <!--    <img src="/img/logo1.png" alt=""> -->
            </div>

            <div id="cabeceraForm">
            </div>
        </div>
    </div>
    <!-- Creacion Menu contextual-->

    <div id="menuContextual">

    </div>

    <div id="impresionFormulario">
        <div id=documentoImpresion>
            <div class="primeraLineaImpresion">

                <div id="comanderaIndivImpresion">
                </div>


            </div>
            <div id="impresionFinal">
                <div class="segundLineaImpresion">
                    <div class="logoFormImpresionDef">
                        <!-- <img src="/img/In-negro.png" alt=""> -->
                    </div>
                    <div class="logoFormImpresion">
                        <!-- <img src="/img/logo1.png" alt=""> -->
                    </div>
                </div>
                <div id=cuerpoImpresion>

                </div>
            </div>
        </div>
    </div>
    <div id="ayuda" class="oculto">
        <div id=documentoAyuda>
            <div id="comanderaIndivAyuda">
            </div>
            <div class="primerLineaAyuda">
                <div class="logoAyuda">
                    <!-- <img src="/img/gesfinPrinc.png" alt="GesFin"> -->
                </div>
            </div>
            <div id=cuerpoAyuda>

            </div>
        </div>
